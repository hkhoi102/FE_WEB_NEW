# Vite environment variables start with VITE_
# Provide a Google Maps embed URL if you want a specific location embedded.
# Get it from Google Maps → Share → Embed a map → Copy HTML → extract the src URL
# Example: https://www.google.com/maps/embed?pb=!1m18!1m12!...
VITE_MAPS_EMBED_URL=
VITE_ABOUT_SUPERMARKET_IMG=
# API Configuration
VITE_API_BASE_URL=http://localhost:8085/api

# App Configuration
VITE_APP_NAME=Smart Web
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false
